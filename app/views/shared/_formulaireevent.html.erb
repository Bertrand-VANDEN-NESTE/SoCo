<div class="container">
  <%= simple_form_for(@event) do |f| %>
    <%= f.input :title %>
    <%= f.input :date %>
    <%= f.input :location %>
    <%= f.input :description %>
    <%= f.input :capacity %>
    <%= f.input :status %>
    <%= f.input :password %>
    <%= f.input :photo,  as: :file %>
    <%= f.submit %>
  <% end %>
</div>
